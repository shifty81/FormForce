{
  "name": "formforce",
  "version": "1.0.0",
  "description": "AI Form Builder - Mobile forms for any device. A mobile and web-based forms solution with dispatching and inventory tracking",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "test": "echo \"Tests will be added\" && exit 0"
  },
  "keywords": [
    "forms",
    "mobile",
    "dispatching",
    "inventory",
    "UETA",
    "electronic-signatures"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0",
    "socket.io": "^4.6.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  }
}
